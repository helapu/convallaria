<h2>Show Verify code</h2>

<ul>

  <li>
    <strong>Verify Code:</strong>
    <%= @verify_code.verify_code %>
  </li>

  <li>
    <strong>Type:</strong>
    <%= @verify_code.type %>
  </li>

  <li>
    <strong>Mobile:</strong>
    <%= @verify_code.mobile %>
  </li>

  <li>
    <strong>Status:</strong>
    <%= @verify_code.status %>
  </li>

  <li>
    <strong>Message:</strong>
    <%= @verify_code.message %>
  </li>

</ul>

<span><%= link "Edit", to: admin_verify_code_path(@conn, :edit, @verify_code) %></span>
<span><%= link "Back", to: admin_verify_code_path(@conn, :index) %></span>
